###########################################################
###################### General Setup ######################
###########################################################

### Drivers ################################################

# All volumes driver
VOLUMES_DRIVER=local

# TimeZone ################################################
TZ=Asia/Shanghai

### Docker Host IP ########################################

# Enter your Docker Host IP (will be appended to /etc/hosts). Default is `10.0.75.1`
DOCKER_HOST_IP=172.19.200.1

# Choose storage path on your machine. For all storage systems
DATA_PATH_HOST=~/.docker-env/elk

# log storage path on your machine.
LOG_PATH_HOST=../logs/elk

# Define the prefix of container names. This is useful if you have multiple projects that use laradock to have separate containers per project.
COMPOSE_PROJECT_NAME=docker-env-elk

### ELK #################################################

ELK_VERSION=8.12.0

### ELASTICSEARCH #########################################

ES_CONTAINER_NAME=elasticsearch
ES_HOSTNAME=elasticsearch
ES_PATH=./elasticsearch
ES_HOST_HTTP_PORT=9200
ES_HOST_TRANSPORT_PORT=9300
ELASTIC_USERNAME=elastic
ES_PASSWORD=changeme
ES_FRONTEND_IP=172.19.140.130
ES_BACKEND_IP=172.19.150.130

### KIBANA ################################################

